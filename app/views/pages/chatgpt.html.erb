<%
  set_meta_tags(
  title: "AI for your Code Queries | LeChariot",
  description: "Get some help with your code queries from our AI powered chatbot.",
  )
%>
<%# AI Stuff %>
<div class="container mt-5">
  <h1>ChadGPT</h1>
  <%=# render partial: "ai/request_form", locals: { uuid: @uuid } %>
  <%= turbo_stream_from "channel_#{@uuid}" %>
  <div id="ai_output">
    <%= render partial: "ai/output", locals: { generated_idea: "" } %>
  </div>
  <div id="ai_output_image">
    <%= render partial: "ai/output_image", locals: { generated_image: "" } %>
  </div>
</div>
